# R mnozina pravidel
{
s1 Agáta -> s2 a Agáta b Bedřich c Cecílie,
s2 Agáta -> s3 a Agáta,
s3 Bedřich -> s4 b Bedřich,
s4 Cecílie -> s2 c Cecílie,
s2 Agáta -> s5 ,
s5 Bedřich -> s5 '' '' '',
s5 Cecílie -> s5 ''
},

# s pocatecni stav
s1,

