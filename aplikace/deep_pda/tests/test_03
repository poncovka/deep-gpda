(
# Q mnozina stavu
{s1, s2, s3, s4, s5},

# Sigma mnozina vstupnich symbolu
{á,ů,ž},

# Gamma mnozina zasobnikovych symbolu
{Agáta,Bedřich,Cecílie, á,ů,ž},

# R mnozina pravidel
{
s1 Agáta -> s2 á Agáta ů Bedřich ž Cecílie,
s2 Agáta -> s3 á Agáta,
s3 Bedřich -> s4 ů Bedřich,
s4 Cecílie -> s2 ž Cecílie,
s2 Agáta -> s5,
s5 Bedřich -> s5 '' '' '',
s5 Cecílie -> s5 ''
},

# s pocatecni stav
s1,

# S pocatecni symbol
